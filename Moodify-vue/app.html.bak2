            emotionValues[3].style.backgroundColor = `rgba(${disgustColor[0]}, ${disgustColor[1]}, ${disgustColor[2]}, 0.8)`;
          }
          
          // æ›´æ–°å€¼æ˜¾ç¤ºé¢æ¿çš„é¢œè‰²
