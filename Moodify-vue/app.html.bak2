        // æ›´æ–°æ»‘å—å€¼æ˜¾ç¤ºçš„é¢œè‰²
        app.methods.updateSliderValueColors = function() {
          // å¿«ä¹å€¼çš„é¢œè‰² - ä»è“è‰²åˆ°é»„è‰²
          let happyColor;
          if (this.happiness > 50) {
            const happyIntensity = (this.happiness - 50) / 50;
            happyColor = this.getGradientColor([255, 255, 255], [241, 196, 15], happyIntensity); // é»„è‰² #f1c40f
          } else {
            const sadIntensity = (50 - this.happiness) / 50;
            happyColor = this.getGradientColor([255, 255, 255], [52, 152, 219], sadIntensity); // è“è‰² #3498db
          }
          
          // ç”Ÿæ°”å€¼çš„é¢œè‰² - ä»ç»¿è‰²åˆ°çº¢è‰²
          let angerColor;
          if (this.anger > 50) {
            const angryIntensity = (this.anger - 50) / 50;
            angerColor = this.getGradientColor([255, 255, 255], [231, 76, 60], angryIntensity); // çº¢è‰² #e74c3c
          } else {
            const fearIntensity = (50 - this.anger) / 50;
            angerColor = this.getGradientColor([255, 255, 255], [46, 204, 113], fearIntensity); // ç»¿è‰² #2ecc71
          }
          
          // æœŸå¾…å€¼çš„é¢œè‰² - ä»é’ç»¿è‰²åˆ°æ©˜è‰²
          let anticipationColor;
          if (this.anticipation > 50) {
            const antIntensity = (this.anticipation - 50) / 50;
            anticipationColor = this.getGradientColor([255, 255, 255], [243, 156, 18], antIntensity); // æ©˜è‰² #f39c12
          } else {
            const surpriseIntensity = (50 - this.anticipation) / 50;
            anticipationColor = this.getGradientColor([255, 255, 255], [26, 188, 156], surpriseIntensity); // é’ç»¿è‰² #1abc9c
          }
          
          // åŒæ¶å€¼çš„é¢œè‰² - ä»è‰ç»¿è‰²åˆ°ç´«è‰²
          let disgustColor;
          if (this.disgust > 50) {
            const disgustIntensity = (this.disgust - 50) / 50;
            disgustColor = this.getGradientColor([255, 255, 255], [142, 68, 173], disgustIntensity); // ç´«è‰² #8e44ad
          } else {
